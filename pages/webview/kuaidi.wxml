<!--pages/webview/kuaidi.wxml-->
<!--index.wxml-->  
    <swiper class="swiper" indicator-dots="true" autoplay="{{autoplay}}" circular="{{circular}}" vertical="{{vertical}}"
        interval="{{interval}}" duration="{{duration}}">  
      <block wx:for="{{movies}}" wx:key="index">  
        <swiper-item>  
          <image src="{{item.url}}" class="slide-image" mode="aspectFill"/>  
        </swiper-item>  
      </block>  
    </swiper> 